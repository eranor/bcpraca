<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="accordion" type="Accordion" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
     <panes>
         <TitledPane text="Modules">
             <AnchorPane>
                 <ScrollPane fx:id="scrollPane" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="-10.0" AnchorPane.leftAnchor="-10.0" AnchorPane.rightAnchor="-10.0" AnchorPane.topAnchor="-10.0">
                     <AnchorPane>
                         <VBox fx:id="modulesContainer" alignment="CENTER" fillWidth="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            </VBox>
                     </AnchorPane>
                 </ScrollPane>
             </AnchorPane>
         </TitledPane>
         <TitledPane animated="true" text="Variables">
             <AnchorPane>
                 <ScrollPane fitToWidth="true" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="-10.0" AnchorPane.leftAnchor="-10.0" AnchorPane.rightAnchor="-10.0" AnchorPane.topAnchor="-10.0">
                     <AnchorPane>
                         <VBox fx:id="variablesContainer" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            </VBox>
                     </AnchorPane>
                 </ScrollPane>
             </AnchorPane>
         </TitledPane>

     </panes>
 </fx:root>
